<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root_dialog_get_success_integral"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="@dimen/dp_267"
        android:layout_height="@dimen/dp_333"
        android:layout_gravity="center"
        android:background="@mipmap/bg_get_success_integral" />

    <TextView
        android:id="@+id/tv_dialog_get_success_integral"
        android:layout_width="@dimen/dp_185"
        android:layout_height="@dimen/dp_44"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@drawable/bg_get_success_btn"
        android:gravity="center"
        android:textColor="@color/white_FFFFFF"
        android:textSize="@dimen/sp_18" />
</LinearLayout>